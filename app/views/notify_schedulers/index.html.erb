<h1>Listing notify_schedulers</h1>

<table>
  <tr>
    <th>Period</th>
    <th>Start at</th>
    <th>Start end</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notify_schedulers.each do |notify_scheduler| %>
  <tr>
    <td><%= notify_scheduler.period %></td>
    <td><%= notify_scheduler.start_at %></td>
    <td><%= notify_scheduler.start_end %></td>
    <td><%= link_to 'Show', notify_scheduler %></td>
    <td><%= link_to 'Edit', edit_notify_scheduler_path(notify_scheduler) %></td>
    <td><%= link_to 'Destroy', notify_scheduler, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

